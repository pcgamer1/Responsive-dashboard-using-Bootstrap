    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">Dashboard</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-left">
                        {{!-- <li>
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-refresh"></i>
                            </a>
                        </li> --}}
                        <li class="sbar">
                            <form class="form-inline my-2 my-lg-0">
                            <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                            {{!-- <button class="btn btn-outline-success my-2 my-sm-0" type="submit"><i type="submit" class="fa fa-search"></i></button> --}}
                            </form>
                        </li>
                    </ul>

                    <ul class="nav navbar-nav navbar-right">
                        <li>
                           <a href="">
                               {{!-- <i class="pe-7s-config pe-4x"></i> --}}
                               {{>userimg data=avatar}}
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>


        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-3 col-sm-6">
                        <div class="card">
                            <div class="row"> <div class="col-md-4">
                            <div class="content">
                                <i class="fas fa-dollar-sign fa-2x" style="padding-left: 26px; padding-right: 26px; padding-top: 20px; padding-bottom:20px; border: 1px solid rgba(14, 189, 14, 0.3); border-radius: 50%; background:rgb(177, 255, 216)"></i>
                            </div>
                        </div>
                        <div class="col-md-8">
                            <div class="content topa">
                                {{totalRevenue}}
                            </div>
                            <div class="content" style="text-align: right; font-size: calc(41px - 4.5vmin); color:rgba(120, 120, 120, .7); font-weight: bold;">Total Revenue</div>
                        </div>
                    </div> </div> </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="card">
                            <div class="row"> 
                                <div class="col-md-4">
                                    <div class="content"> 
                                        <i class="fas fa-cart-plus fa-2x" style="padding: 20px; border: 1px solid rgba(194, 13, 194, 0.3); border-radius: 50%; background:rgb(227, 203, 255);"></i>
                                    </div>
                                </div>
                                <div class="col-md-8">
                                    <div class="content topa">
                                        {{todaysSales}}
                                    </div>
                                    <div class="content" style="text-align: right; font-size: calc(41px - 4.5vmin); color:rgba(120, 120, 120, .7); font-weight: bold;">Today's Sales</div>
                                </div>
                            </div> 
                        </div> 
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="card">
                            <div class="row"> <div class="col-md-4">
                            <div class="content">
                                <i class="fas fa-chart-bar fa-2x" style="padding: 20px; border: 1px solid rgba(14, 189, 14, 0.3); border-radius: 50%; background: rgb(182, 255, 219)"></i>
                            </div>
                        </div>
                        <div class="col-md-8">
                            <div class="content topa">
                                {{conversion}}
                            </div>
                            <div class="content" style="text-align: right; font-size: calc(41px - 4.5vmin); color:rgba(120, 120, 120, .7); font-weight: bold;">Conversion</div>
                        </div>
                    </div> </div> </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="card">
                            <div class="row"> <div class="col-md-4">
                            <div class="content">
                                <i class="fas fa-eye fa-2x" style="padding: 20px; border: 1px solid rgba(194, 13, 194, 0.3); border-radius: 50%; background: rgb(222, 194, 255)"></i>
                            </div>
                        </div>
                        <div class="col-md-8">
                            <div class="content topa">
                                {{todaysVisits}}
                            </div>
                            <div class="content" style="text-align: right; font-size: calc(41px - 4.5vmin); color: rgba(120, 120, 120, .7); font-weight: bold;">Today's Visits</div>
                        </div>
                    </div> </div> </div>
                </div>
       
                <div class="row">
                    <div class="col-md-4">
                        <div class="card card-revenue">
                            <div class="header">
                                <p class="subhead">TOTAL REVENUE</p>
                            </div>
                            <div class="content">
                                <div class="row">
                                    <div class="progress blue">
                                        <span class="progress-left">
                                            <span class="progress-bar"></span>
                                        </span>
                                        <span class="progress-right">
                                            <span class="progress-bar"></span>
                                        </span>
                                        <div class="progress-value">90%</div>
                                    </div> 
                                </div>
                                <div class="row">
                                    <div class="content">
                                        <h5 class="title" style="color: rgba(120, 120, 120, .6); font-weight: bold; font-size: small;">Total sales made today</h5>
                                        <p></p>
                                        <p class="amount">$75</p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="content">
                                        <div class="col-md-4">
                                            <h5 class="title" style="color: rgba(120, 120, 120, .6); font-weight: bold; font-size: small;">Target</h5>
                                            <p></p>
                                            <p class="amountl">$1000</p>
                                        </div>
                                        <div class="col-md-4">
                                            <h5 class="title" style="color: rgba(120, 120, 120, .6); font-weight: bold; font-size: small;">Last week</h5>
                                            <p></p>
                                            <p class="amountl">$523</p>
                                        </div>
                                        <div class="col-md-4">
                                            <h5 class="title" style="color: rgba(120, 120, 120, .6); font-weight: bold; font-size: small;">Last month</h5>
                                            <p></p>
                                            <p class="amountl">$965</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-8">
                        <div class="card">
                            <div class="header">
                                <p class="subhead">TOTAL SALES</p>
                            </div>
                            {{!-- <div class="content"> --}}
                                    <canvas id="lineChart"></canvas>
                                    <script src="/js/plot.js"></script>
                            {{!-- </div> --}}
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="card card-sales">
                            <div class="header">
                                <p class="subhead">SALES ANALYTICS</p>
                            </div>
                            <canvas id="barChart"></canvas>
                            {{!-- <script src="/js/bar.js"></script> --}}
                            <script src="/js/bar.js"></script>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card card-orders">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="header">
                                        <p class="subhead">RECENT ORDERS</p>
                                    </div>
                                </div>
                                <div class="col-md-6 top">
                                    <button class="btn btn-info btn-fill btn-sm">View All</button>
                                </div>
                            </div>
    
                            <div class="content">
                            <div class="table-responsive table-full-width">
	                            <table class="table table-hover table-striped">
		                            <thead>
			                            <th>Product</th>
                                        <th>Order Date</th>
                                        <th>Order Number</th>
                                        <th>Amount</th>
                                        <th>Customer Name</th>
                                    </thead>
                                    <tbody>
                                        {{>rows data=[0]}} {{>rows data=[1]}} {{>rows data=[2]}} {{>rows data=[3]}} {{>rows data=[4]}}
                                    </tbody>
	                            </table>
                            </div></div>
                            <div></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <footer class="footer">
            <div class="container-fluid">
                <nav class="pull-left">
                    <ul>
                        <li>
                            <a href="#">
                                Home
                            </a>
                        </li>

                    </ul>
                </nav>
                <p class="copyright pull-right">
                    &copy; <script>document.write(new Date().getFullYear())</script> <a>Sarthak Saxena</a>
                </p>
            </div>
        </footer>

    </div>
</div>

    </body>
